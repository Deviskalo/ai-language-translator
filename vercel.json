{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "DATABASE_URL": "@DATABASE_URL",
    "GMAIL_USER": "@GMAIL_USER",
    "GMAIL_PASS": "@GMAIL_PASS"
  },
  "build": {
    "env": {
      "DATABASE_URL": "@DATABASE_URL",
      "GMAIL_USER": "@GMAIL_USER",
      "GMAIL_PASS": "@GMAIL_PASS"
    }
  }
}