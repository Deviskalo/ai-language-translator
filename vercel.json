{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "GMAIL_USER": "@gmail_user",
    "GMAIL_PASS": "@gmail_pass"
  },
  "build": {
    "env": {
      "DATABASE_URL": "@database_url",
      "GMAIL_USER": "@gmail_user",
      "GMAIL_PASS": "@gmail_pass"
    }
  }
}